@model List<MVC.Models.BookModel>
@{
    ViewData["Title"] = "BookList";
}

<h1>Book List</h1>

<div class="books">


    @foreach (var book in Model)
    {
        <div class="book">
            <h1>@book.BookTitle</h1>
            <h2>@book.Author</h2>
            <p>@book.Year</p>
            <span>@book.Rating</span>
            <p>@(book.HaveRead ? "Read" : "Not read")</p>
        </div>
    }

</div>